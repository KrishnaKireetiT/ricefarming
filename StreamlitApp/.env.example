# Environment Variables Template
# Copy this file to .env and fill in your values

# ========================================
# Qwen LLM Configuration
# ========================================
QWEN_API_KEY=your_qwen_api_key_here
QWEN_BASE_URL=http://hanoi2.ucd.ie/v1
QWEN_MODEL_NAME=cpatonn/Qwen3-30B-A3B-Instruct-2507-AWQ-4bit

# ========================================
# Neo4j Database
# ========================================
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_password_here
NEO4J_DATABASE=neo4j

# ========================================
# Langfuse Tracing
# ========================================
LANGFUSE_PUBLIC_KEY=your_public_key_here
LANGFUSE_SECRET_KEY=your_secret_key_here
LANGFUSE_HOST=https://cloud.langfuse.com
LANGFUSE_TIMEOUT=10

# ========================================
# Embedding Model
# ========================================
EMBEDDING_MODEL_NAME=BAAI/bge-base-en-v1.5

# ========================================
# Search Configuration
# ========================================
ENTITY_ALIGNMENT_THRESHOLD=0.6
VECTOR_SEARCH_K=5
KEYWORD_SEARCH_K=3
GRAPH_TRAVERSAL_TOP_K=20

# ========================================
# App Settings
# ========================================
APP_SECRET_KEY=generate_a_random_secret_key_here
